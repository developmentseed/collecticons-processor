<% var icons = [] %>
<% _.each(font.glyphs, function(glyph) {
  icons.push({
    icon: className + '-' + glyph.name,
    charCode: glyph.unicode[0].charCodeAt(0).toString(16).toUpperCase()
  });
}); %>
{
  "name": "<%= font.name %>",
  "version": "<%= version %>",
  "className": "<%= className %>",
  "icons": <%= JSON.stringify(icons) %>
}

